<html>
<head>
    <title>tuxharness - harness routes</title>
</head>
<body>
    <h2>Your Routes</h2>
    {#harnesses}
        <ul>
            <li>Route {#missingRoute}{route}{:else}<a href="{route}">{route}{/missingRoute}</a></li>
            <li>View {view.path}</li>
            <li>Data {datum} {#missingData}{:else}<a href="/json?route={route}">Examine JSON</a>{/missingData}</li>
        </ul>
    {/harnesses}
    {?register.static}
        <h2>Static Route</h2>
        <ul>
            {#register.static.route}
                <li><a href="{.}">{.}</a> points to {register.static.directory}</li>
            {/register.static.route}
        </ul>
    {/register.static}
</body>
</html>
