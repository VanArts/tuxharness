<html>
<head>
    <title>tuxharness JSON structure</title>
</head>
<body>
    <h2>Your Routes</h2>
    {#harnesses}
        <ul>
            <li>Route <a href="{route}">{route}</a></li>
            <li>View {view.path}</li>
            <li>Data {datum}</li>
        </ul>
    {/harnesses}
    {?register.static}
        <h2>Static Route</h2>
        <ul>
            {#register.static.route}
                <li><a href="{.}">{.}</a> points to {register.static.directory}</li>
            {/register.static.route}
        </ul>
    {/register.static}
</body>
</html>
